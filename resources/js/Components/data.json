  {
    "name": "Table",
    "code": "<div class=\"w-full max-w-4xl mx-auto bg-white shadow-lg rounded-lg overflow-hidden\">\n  <table class=\"w-full\">\n    <thead class=\"bg-gray-100 border-b\">\n      <tr>\n        <th class=\"px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Name</th>\n        <th class=\"px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Email</th>\n        <th class=\"px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Role</th>\n        <th class=\"px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Status</th>\n        <th class=\"px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Department</th>\n        <th class=\"px-6 py-4 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">Actions</th>\n      </tr>\n    </thead>\n    <tbody class=\"divide-y divide-gray-200\">\n      <tr class=\"hover:bg-gray-50 transition duration-200\">\n        <td class=\"px-6 py-4 whitespace-nowrap flex items-center space-x-3\">\n          <div class=\"w-10 h-10 bg-gray-300 rounded-full flex items-center justify-center text-gray-500\">\n            <svg class=\"w-6 h-6\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n              <path fill-rule=\"evenodd\" d=\"M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z\" clip-rule=\"evenodd\"/>\n            </svg>\n          </div>\n          <div>\n            <div class=\"text-sm font-medium text-gray-900\">John Doe</div>\n          </div>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">john@example.com</td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n          <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800\">Admin</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n  <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-indigo-100 text-green-800 \">Active</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">IT</td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\n          <div class=\"flex justify-end space-x-2\">\n            <button class=\"text-indigo-600 hover:text-indigo-900\">Edit</button>\n            <button class=\"text-red-600 hover:text-red-900\">Delete</button>\n          </div>\n        </td>\n      </tr>\n      <tr class=\"hover:bg-gray-50 transition duration-200\">\n        <td class=\"px-6 py-4 whitespace-nowrap flex items-center space-x-3\">\n          <div class=\"w-10 h-10 bg-gray-300 rounded-full flex items-center justify-center text-gray-500\">\n            <svg class=\"w-6 h-6\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n              <path fill-rule=\"evenodd\" d=\"M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z\" clip-rule=\"evenodd\"/>\n            </svg>\n          </div>\n          <div>\n            <div class=\"text-sm font-medium text-gray-900\">Jane Smith</div>\n          </div>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">jane@example.com</td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n          <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-blue-100 text-blue-800\">Editor</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n          <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-yellow-100 text-yellow-800\">Pending</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">Marketing</td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\n          <div class=\"flex justify-end space-x-2\">\n            <button class=\"text-indigo-600 hover:text-indigo-900\">Edit</button>\n            <button class=\"text-red-600 hover:text-red-900\">Delete</button>\n          </div>\n        </td>\n      </tr>\n      <tr class=\"hover:bg-gray-50 transition duration-200\">\n        <td class=\"px-6 py-4 whitespace-nowrap flex items-center space-x-3\">\n          <div class=\"w-10 h-10 bg-gray-300 rounded-full flex items-center justify-center text-gray-500\">\n            <svg class=\"w-6 h-6\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n              <path fill-rule=\"evenodd\" d=\"M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z\" clip-rule=\"evenodd\"/>\n            </svg>\n          </div>\n          <div>\n            <div class=\"text-sm font-medium text-gray-900\">Mike Johnson</div>\n          </div>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">mike@example.com</td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n          <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-purple-100 text-purple-800\">Manager</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap\">\n          <span class=\"px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800\">Active</span>\n        </td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">HR</td>\n        <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\n          <div class=\"flex justify-end space-x-2\">\n            <button class=\"text-indigo-600 hover:text-indigo-900\">Edit</button>\n            <button class=\"text-red-600 hover:text-red-900\">Delete</button>\n          </div>\n        </td>\n      </tr>\n    </tbody>\n  </table>\n</div>"
  },
  
  {
    "name": "Sign In",
    "code": "<div class=\"min-h-screen bg-gray-100 flex flex-col justify-center py-12 sm:px-6 lg:px-8\">\n  <div class=\"sm:mx-auto sm:w-full sm:max-w-md\">\n    <div class=\"bg-white py-8 px-4 shadow sm:rounded-lg sm:px-10\">\n      <div class=\"sm:mx-auto sm:w-full sm:max-w-md mb-8\">\n        <h2 class=\"text-center text-3xl font-extrabold text-gray-900\">\n          Sign in to your account\n        </h2>\n      </div>\n      <form class=\"space-y-6\">\n        <div>\n          <label for=\"email\" class=\"block text-sm font-medium text-gray-700\">\n            Email address\n          </label>\n          <div class=\"mt-1\">\n            <input id=\"email\" name=\"email\" type=\"email\" autocomplete=\"email\" required class=\"appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\">\n          </div>\n        </div>\n        <div>\n          <label for=\"password\" class=\"block text-sm font-medium text-gray-700\">\n            Password\n          </label>\n          <div class=\"mt-1\">\n            <input id=\"password\" name=\"password\" type=\"password\" autocomplete=\"current-password\" required class=\"appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\">\n          </div>\n        </div>\n        <div class=\"flex items-center justify-between\">\n          <div class=\"flex items-center\">\n            <input id=\"remember-me\" name=\"remember-me\" type=\"checkbox\" class=\"h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded\">\n            <label for=\"remember-me\" class=\"ml-2 block text-sm text-gray-900\">\n              Remember me\n            </label>\n          </div>\n          <div class=\"text-sm\">\n            <a href=\"#\" class=\"font-medium text-indigo-600 hover:text-indigo-500\">\n              Forgot your password?\n            </a>\n          </div>\n        </div>\n        <div>\n          <button type=\"submit\" class=\"w-full flex justify-center py-2 px-4 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500\">\n            Sign in\n          </button>\n        </div>\n      </form>\n      <div class=\"mt-6\">\n        <div class=\"relative\">\n          <div class=\"absolute inset-0 flex items-center\">\n            <div class=\"w-full border-t border-gray-300\"></div>\n          </div>\n          <div class=\"relative flex justify-center text-sm\">\n            <span class=\"px-2 bg-white text-gray-500\">\n              Or continue with\n            </span>\n          </div>\n        </div>\n        <div class=\"mt-6 grid grid-cols-3 gap-3\">\n          <div>\n            <a href=\"#\" class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\">\n              Google\n            </a>\n          </div>\n          <div>\n            <a href=\"#\" class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\">\n              Facebook\n            </a>\n          </div>\n          <div>\n            <a href=\"#\" class=\"w-full inline-flex justify-center py-2 px-4 border border-gray-300 rounded-md shadow-sm bg-white text-sm font-medium text-gray-500 hover:bg-gray-50\">\n              GitHub\n            </a>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>"
  },
  {
    "name": "Notifications",
    "code": "<div class=\"container mx-auto px-4 py-8 space-y-6\">\n  <!-- Success Notification -->\n  <div class=\"bg-green-100 rounded-lg shadow-md p-6 flex items-center\">\n    <svg class=\"w-8 h-8 text-green-500 mr-4\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z\" />\n    </svg>\n    <div>\n      <p class=\"text-green-800 font-bold text-lg\">Success Notification</p>\n      <p class=\"text-green-600\">Action completed successfully!</p>\n    </div>\n  </div>\n\n  <!-- Error Notification -->\n  <div class=\"bg-red-100 rounded-lg shadow-md p-6 flex items-center\">\n    <svg class=\"w-8 h-8 text-red-500 mr-4\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z\" />\n    </svg>\n    <div>\n      <p class=\"text-red-800 font-bold text-lg\">Error Notification</p>\n      <p class=\"text-red-600\">Something went wrong. Please try again.</p>\n    </div>\n  </div>\n\n  <!-- Warning Notification -->\n  <div class=\"bg-yellow-100 rounded-lg shadow-md p-6 flex items-center\">\n    <svg class=\"w-8 h-8 text-yellow-500 mr-4\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 8v4m0 4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z\" />\n    </svg>\n    <div>\n      <p class=\"text-yellow-800 font-bold text-lg\">Warning Notification</p>\n      <p class=\"text-yellow-600\">Proceed with caution.</p>\n    </div>\n  </div>\n\n  <!-- Information Notification -->\n  <div class=\"bg-blue-100 rounded-lg shadow-md p-6 flex items-center\">\n    <svg class=\"w-8 h-8 text-blue-500 mr-4\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z\" />\n    </svg>\n    <div>\n      <p class=\"text-blue-800 font-bold text-lg\">Information Notification</p>\n      <p class=\"text-blue-600\">Here's some important information.</p>\n    </div>\n  </div>\n\n  <!-- Notification Alert -->\n  <div class=\"bg-purple-100 rounded-lg shadow-md p-6 flex items-center\">\n    <svg class=\"w-8 h-8 text-purple-500 mr-4\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.58V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.586c0 .53-.21 1.04-.586 1.414L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9\" />\n    </svg>\n    <div>\n      <p class=\"text-purple-800 font-bold text-lg\">Notification Alert</p>\n      <p class=\"text-purple-600\">You have a new notification!</p>\n    </div>\n  </div>\n</div>"
  },
  {
    "name": "ProductDetails",
    "code": "<div class=\"container mx-auto px-4 py-8 max-w-6xl\">\n  <div class=\"grid md:grid-cols-2 gap-8 items-center\">\n    <div class=\"bg-gray-200 flex items-center justify-center h-96\">\n      <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-16 w-16 text-gray-500\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n        <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 16l4.586-4.586a2 2 0 012.828 0L16 16m-2-2l1.586-1.586a2 2 0 012.828 0L20 14m-6-6h.01M6 20h12a2 2 0 002-2V6a2 2 0 00-2-2H6a2 2 0 00-2 2v12a2 2 0 002 2z\" />\n      </svg>\n    </div>\n    <div>\n      <h1 class=\"text-3xl font-bold text-gray-900 mb-4\">Classic T-Shirt</h1>\n      <div class=\"flex items-center mb-4\">\n        <span class=\"text-2xl font-semibold text-blue-600 mr-4\">$29.99</span>\n        <div class=\"flex\">\n          <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-5 w-5 text-yellow-500\" viewBox=\"0 0 20 20\" fill=\"currentColor\">\n            <path d=\"M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z\" />\n          </svg>\n          <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-5 w-5 text-yellow-500\" viewBox=\"0 0 20 20\" fill=\"currentColor\">\n            <path d=\"M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z\" />\n          </svg>\n          <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-5 w-5 text-yellow-500\" viewBox=\"0 0 20 20\" fill=\"currentColor\">\n            <path d=\"M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z\" />\n          </svg>\n          <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-5 w-5 text-gray-300\" viewBox=\"0 0 20 20\" fill=\"currentColor\">\n            <path d=\"M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z\" />\n          </svg>\n          <span class=\"ml-2 text-gray-600\">(128 reviews)</span>\n        </div>\n      </div>\n      <p class=\"text-gray-700 mb-6\">Soft, comfortable, and versatile. Our classic t-shirt is perfect for any casual occasion.</p>\n      <div class=\"mb-6\">\n        <h3 class=\"text-lg font-semibold mb-2\">Color</h3>\n        <div class=\"flex space-x-3\">\n          <button class=\"w-8 h-8 rounded-full bg-gray-800\"></button>\n          <button class=\"w-8 h-8 rounded-full bg-blue-500\"></button>\n          <button class=\"w-8 h-8 rounded-full bg-green-500\"></button>\n        </div>\n      </div>\n      <div class=\"mb-6\">\n        <h3 class=\"text-lg font-semibold mb-2\">Size</h3>\n        <div class=\"flex space-x-2\">\n          <button class=\"px-4 py-2 border rounded hover:bg-gray-100\">S</button>\n          <button class=\"px-4 py-2 border rounded hover:bg-gray-100\">M</button>\n          <button class=\"px-4 py-2 border rounded hover:bg-gray-100\">L</button>\n          <button class=\"px-4 py-2 border rounded hover:bg-gray-100\">XL</button>\n        </div>\n      </div>\n      <div class=\"flex space-x-4\">\n        <button class=\"bg-blue-600 text-white px-6 py-3 rounded hover:bg-blue-700 transition duration-300\">Add to Cart</button>\n        <button class=\"border border-gray-300 text-gray-700 px-6 py-3 rounded hover:bg-gray-100 transition duration-300\">Buy Now</button>\n      </div>\n    </div>\n  </div>\n</div>"
  },
  {
    "name": "OrderHistory",
    "code": "<div class=\"container mx-auto px-4 py-8 bg-gray-50\">\n  <div class=\"max-w-4xl mx-auto\">\n    <h1 class=\"text-3xl font-bold text-gray-800 mb-6\">Order History</h1>\n\n    <div class=\"bg-white shadow-md rounded-lg overflow-hidden\">\n      <div class=\"px-6 py-4 border-b border-gray-200\">\n        <div class=\"flex justify-between items-center\">\n          <div>\n            <p class=\"text-sm text-gray-600\">Order #12345</p>\n            <p class=\"text-sm text-gray-500\">Placed on March 15, 2024</p>\n          </div>\n          <span class=\"px-3 py-1 bg-green-100 text-green-800 rounded-full text-xs\">Delivered</span>\n        </div>\n      </div>\n\n      <div class=\"p-6\">\n        <div class=\"flex items-center space-x-4 mb-4\">\n          <div class=\"w-24 h-24 bg-gray-200 flex items-center justify-center text-gray-500\">\n            Product Image\n          </div>\n          <div class=\"flex-1\">\n            <h2 class=\"text-lg font-semibold text-gray-800\">Wireless Headphones</h2>\n            <p class=\"text-sm text-gray-600\">Color: Black | Size: One Size</p>\n            <p class=\"text-sm text-gray-600\">Quantity: 1</p>\n          </div>\n          <div class=\"text-right\">\n            <p class=\"font-semibold text-gray-800\">$129.99</p>\n          </div>\n        </div>\n\n        <div class=\"flex items-center space-x-4\">\n          <div class=\"w-24 h-24 bg-gray-200 flex items-center justify-center text-gray-500\">\n            Product Image\n          </div>\n          <div class=\"flex-1\">\n            <h2 class=\"text-lg font-semibold text-gray-800\">Bluetooth Speaker</h2>\n            <p class=\"text-sm text-gray-600\">Color: Blue | Size: Medium</p>\n            <p class=\"text-sm text-gray-600\">Quantity: 2</p>\n          </div>\n          <div class=\"text-right\">\n            <p class=\"font-semibold text-gray-800\">$79.50</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"bg-gray-100 px-6 py-4 border-t border-gray-200\">\n        <div class=\"flex justify-between\">\n          <p class=\"text-sm text-gray-600\">Subtotal</p>\n          <p class=\"text-sm font-semibold text-gray-800\">$288.49</p>\n        </div>\n        <div class=\"flex justify-between\">\n          <p class=\"text-sm text-gray-600\">Shipping</p>\n          <p class=\"text-sm font-semibold text-gray-800\">$10.00</p>\n        </div>\n        <div class=\"flex justify-between\">\n          <p class=\"text-sm font-bold text-gray-800\">Total</p>\n          <p class=\"text-lg font-bold text-gray-800\">$298.49</p>\n        </div>\n      </div>\n\n      <div class=\"p-6 bg-white border-t border-gray-200\">\n        <div class=\"grid grid-cols-2 gap-4\">\n          <div>\n            <h3 class=\"text-sm font-semibold text-gray-800 mb-2\">Shipping Address</h3>\n            <p class=\"text-sm text-gray-600\">\n              John Doe<br>\n              123 Main Street<br>\n              Anytown, State 12345<br>\n              United States\n            </p>\n          </div>\n          <div>\n            <h3 class=\"text-sm font-semibold text-gray-800 mb-2\">Payment Method</h3>\n            <p class=\"text-sm text-gray-600\">\n              Visa **** 4567<br>\n              Expires 12/2026\n            </p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>"
  },
  {
    "name": "Checkout",
    "code": "<div class=\"min-h-screen bg-gray-100 flex items-center justify-center py-12 px-4 sm:px-6 lg:px-8\">\n  <div class=\"max-w-4xl w-full bg-white shadow-lg rounded-lg overflow-hidden flex\">\n    <div class=\"w-1/2 bg-gray-200 flex items-center justify-center\">\n      <div class=\"text-gray-500 text-center\">\n        <svg class=\"mx-auto h-20 w-20\" fill=\"currentColor\" viewBox=\"0 0 24 24\">\n          <path d=\"M16 6l2.29 2.29-4.88 4.88-4-4L2 16.59 3.41 18l6-6 4 4 6.3-6.29L22 12V6z\"/>\n        </svg>\n        <p class=\"mt-2 text-sm\">Product Image Placeholder</p>\n      </div>\n    </div>\n    <div class=\"w-1/2 p-8\">\n      <h2 class=\"text-3xl font-extrabold text-gray-900 mb-6\">Checkout</h2>\n      <form class=\"space-y-6\">\n        <div>\n          <label for=\"email\" class=\"block text-sm font-medium text-gray-700\">Email address</label>\n          <input type=\"email\" id=\"email\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n        </div>\n        <div>\n          <label for=\"name\" class=\"block text-sm font-medium text-gray-700\">Full Name</label>\n          <input type=\"text\" id=\"name\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n        </div>\n        <div class=\"grid grid-cols-2 gap-4\">\n          <div>\n            <label for=\"city\" class=\"block text-sm font-medium text-gray-700\">City</label>\n            <input type=\"text\" id=\"city\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n          </div>\n          <div>\n            <label for=\"zip\" class=\"block text-sm font-medium text-gray-700\">ZIP Code</label>\n            <input type=\"text\" id=\"zip\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n          </div>\n        </div>\n        <div>\n          <label for=\"card\" class=\"block text-sm font-medium text-gray-700\">Credit Card Number</label>\n          <input type=\"text\" id=\"card\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n        </div>\n        <div class=\"grid grid-cols-3 gap-4\">\n          <div>\n            <label for=\"expiry\" class=\"block text-sm font-medium text-gray-700\">Expiry</label>\n            <input type=\"text\" id=\"expiry\" placeholder=\"MM/YY\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n          </div>\n          <div>\n            <label for=\"cvv\" class=\"block text-sm font-medium text-gray-700\">CVV</label>\n            <input type=\"text\" id=\"cvv\" required class=\"mt-1 block w-full border border-gray-300 rounded-md shadow-sm py-2 px-3 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500\"/>\n          </div>\n        </div>\n        <div>\n          <button type=\"submit\" class=\"w-full flex justify-center py-3 px-4 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500\">\n            Complete Purchase\n          </button>\n        </div>\n      </form>\n    </div>\n  </div>\n</div>"
  },
